    "browser-sync": "^2.27.3",
    "cross-env": "^7.0.3",
    "fast-glob": "^3.2.7",
    "generate-source-map": "^0.0.5",
    "node": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.4",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
