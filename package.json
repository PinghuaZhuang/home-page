    "rollup-plugin-require-context": "^1.0.1",
    "rollup-plugin-sass": "^1.2.8",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svg-sprite-loader": "0.0.4",
    "rollup-plugin-uglify": "^6.0.4",
    "sass": "^1.26.3",
